		<script>
			setTimeout(() => {
				const footerBtns = document.querySelectorAll('#section-footer .Linklist a');
				footerBtns.forEach(btn => {
					btn.addEventListener('click', (e) => {
						if (btn.innerHTML == 'Accessibility') {
							e.preventDefault();
							const triggerBtn = document.querySelector('.acsb-trigger.acsb-bg-lead');
							if (triggerBtn) {
								triggerBtn.click();
							}
						}
					});
				});
			}, 2000);
		</script>
