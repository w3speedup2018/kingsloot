	<body class="js-page" itemscope itemtype="http://schema.org/WebPage">
		{{ settings.body_scripts }}
		<a class="sr-only" href="#MainContent">
			{{ "accessibility.skip_to_text" | t }}
		</a>
		<header class="c-header  js-header">
			{%- liquid
				section 'header_announcement-bar'
				section 'header'
			-%}
		</header>
		{% section 'mobile_header_addtion'%}
		<main role="main" tabindex="-1">
			{% comment %}
			{% render 'theme_breadcrumbs' %}
			{% endcomment %}
			{%- if request.design_mode or template contains 'cart' or template contains 'checkout' -%}
				{{ content_for_layout }}
			{%- else -%}
				{% capture content_for_layout2 %}{{ content_for_layout }}{% endcapture %}
				{{ content_for_layout2 | replace: 'src="https://www.youtube.com/', 'data-src="https://www.youtube.com/' | replace: '<link href="//cdn1.stamped.io/' , '<link data-href="//cdn1.stamped.io/' | replace: 'src="chrome-extension://hkifaniplmigcbkcppibejobhicmkjhf/img/icon.png"', ''  }}
			{%- endif -%}
		</main>
		{% section 'footer' %}
		<div class="nav-overlay"></div>
		<ul hidden>
			<li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
		</ul>
		{%- liquid
			render 'model_main'
			section 'mini-basket'
			section 'swatches'
			section 'app-optimisation'
		-%}
		<script>
			window.shopUrl = '{{ request.origin }}';
			window.routes = {
				cart_add_url: '{{ routes.cart_add_url }}',
				cart_change_url: '{{ routes.cart_change_url }}',
				cart_update_url: '{{ routes.cart_update_url }}',
				predictive_search_url: '{{ routes.predictive_search_url }}'
			};
			window.cartStrings = {
				error: `{{ 'sections.cart.cart_error' | t }}`,
				quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
			}
			window.variantStrings = {
				addToCart: `{{ 'products.product.add_to_cart' | t }}`,
				soldOut: `{{ 'products.product.sold_out' | t }}`,
				unavailable: `{{ 'products.product.unavailable' | t }}`,
				moneyFormat: `{{ shop.money_format | json }}`,
			}
		</script>
		{% if template contains 'product' %}
			{% render 'model_customize' %}
			<script>
				const videoBtns = document.querySelectorAll('.c-image_slider-item');
				videoBtns.forEach(btn => {
					btn.addEventListener('click', (e) => {
						if (btn.getAttribute("data-video")) {
							e.preventDefault();
							document.getElementById(btn.getAttribute("data-video")).classList.add('active');
							const modalvideo = document.getElementById(btn.getAttribute("data-video")).querySelector('iframe');
							modalvideo.src += "&autoplay=1"
						}
					})
				});
				const videoClose = document.querySelectorAll('.c-modal-action__close');
				videoClose.forEach(btn => {
					btn.addEventListener('click', (e) => {
					document.querySelectorAll('.c-modal-action').forEach(video => {
							video.classList.remove('active');
							const modalvideo = video.querySelector('iframe');
							var iframeSrc = modalvideo.src;
							modalvideo.src = iframeSrc;
						});
					})
				});
				const video1Close = document.querySelectorAll('.c-modal-action__close-btn');
				video1Close.forEach(btn => {
					btn.addEventListener('click', (e) => {
                        document.querySelectorAll('.c-modal-action').forEach(video => {
							video.classList.remove('active');
						});
					});
				});
			</script>
		{% endif %}
